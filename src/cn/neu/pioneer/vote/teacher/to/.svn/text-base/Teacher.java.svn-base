package cn.neu.pioneer.vote.teacher.to;

import java.util.List;

import cn.neu.pioneer.vote.message.to.Message;
import cn.neu.pioneer.vote.vote.to.Vote;

/**
 * @author changer
 *
 * Dec 8, 2009
 */
public class Teacher {
	private String 				teacherId;
	private String 				teacherName;
	private String 				teacherGender;
	private String 				teacherBirth;  
	private String 				politicalStatus;   //政治面貌
	private String 				recommendUnit;	  //推荐单位
	private String 				unit;			 //所属单位
	private String 				position;		 //职位
	private String 				title;		     //职称
	private String 				teacherType;    //参评类别
	
	private String 				experience;       //工作经历及任职情况
	private String 				outcomes;        //教学及科研成果
	private String 				deeds; 			//老师事迹
	private String 				comments; 		//学生评语
	
	private String 				pic;             //图像保存位置
	
	private List<TeachInfor>	infors;          //教学经历
	private List<Vote>			votes;		     //所得投票
	private List<Message>		messages;		 //所得留言
	
	public String getTeacherId() {
		return teacherId;
	}
	public void setTeacherId(String teacherId) {
		this.teacherId = teacherId;
	}
	public String getTeacherName() {
		return teacherName;
	}
	public void setTeacherName(String teacherName) {
		this.teacherName = teacherName;
	}
	public String getTeacherGender() {
		return teacherGender;
	}
	public void setTeacherGender(String teacherGender) {
		this.teacherGender = teacherGender;
	}
	public String getTeacherBirth() {
		return teacherBirth;
	}
	public void setTeacherBirth(String teacherBirth) {
		this.teacherBirth = teacherBirth;
	}
	public String getPoliticalStatus() {
		return politicalStatus;
	}
	public void setPoliticalStatus(String politicalStatus) {
		this.politicalStatus = politicalStatus;
	}
	public String getRecommendUnit() {
		return recommendUnit;
	}
	public void setRecommendUnit(String recommendUnit) {
		this.recommendUnit = recommendUnit;
	}
	public String getUnit() {
		return unit;
	}
	public void setUnit(String unit) {
		this.unit = unit;
	}
	public String getPosition() {
		return position;
	}
	public void setPosition(String position) {
		this.position = position;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getTeacherType() {
		return teacherType;
	}
	public void setTeacherType(String teacherType) {
		this.teacherType = teacherType;
	}
	public String getExperience() {
		return experience;
	}
	public void setExperience(String experience) {
		this.experience = experience;
	}
	public String getOutcomes() {
		return outcomes;
	}
	public void setOutcomes(String outcomes) {
		this.outcomes = outcomes;
	}
	public String getDeeds() {
		return deeds;
	}
	public void setDeeds(String deeds) {
		this.deeds = deeds;
	}
	public String getComments() {
		return comments;
	}
	public void setComments(String comments) {
		this.comments = comments;
	}
	public String getPic() {
		return pic;
	}
	public void setPic(String pic) {
		this.pic = pic;
	}
	public List<TeachInfor> getInfors() {
		return infors;
	}
	public void setInfors(List<TeachInfor> infors) {
		this.infors = infors;
	}
	public List<Vote> getVotes() {
		return votes;
	}
	public void setVotes(List<Vote> votes) {
		this.votes = votes;
	}
	public List<Message> getMessages() {
		return messages;
	}
	public void setMessages(List<Message> messages) {
		this.messages = messages;
	}
	
	
}
